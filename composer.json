{
  "name": "orchid/platform",
  "description": "The Laravel CMS for website.",
  "keywords": [
    "framework",
    "laravel",
    "orchid",
    "crud",
    "cms",
    "cmf",
    "access",
    "alert",
    "attributes",
    "log",
    "menu",
    "scaffold",
    "seo",
    "settings",
    "tags",
    "widget",
    "website",
    "user",
    "editor",
    "permissions",
    "dashboard",
    "active",
    "admin",
    "bootstrap",
    "platform"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://theorchid.github.io/",
  "support": {
    "issues": "https://github.com/theOrchid/Platform/issues",
    "source": "https://github.com/theOrchid/Platform"
  },
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.*",
    "watson/active": "^2.0",
    "intervention/image": "^2.3",
    "cartalyst/tags": "^4.0",
    "cviebrock/eloquent-sluggable": "^4.2",
    "spatie/laravel-backup": "4.*",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9.4",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "files": [
      "src/Setting/helpers.php",
      "src/Alert/helpers.php",
      "src/Log/helpers.php"
    ],
    "psr-4": {
      "Orchid\\": "app/",
      "Orchid\\Access\\": "src/Access/",
      "Orchid\\Alert\\": "src/Alert/",
      "Orchid\\Field\\": "src/Field/",
      "Orchid\\Forms\\": "src/Forms/",
      "Orchid\\Log\\": "src/Log/",
      "Orchid\\Menu\\": "src/Menu/",
      "Orchid\\Setting\\": "src/Setting/",
      "Orchid\\Behaviors\\": "src/Behaviors/",
      "Orchid\\Widget\\": "src/Widget/",
      "Orchid\\Defender\\": "src/Defender/",
      "Orchid\\Schema\\": "src/Schema/",
      "Orchid\\Monitor\\": "src/Monitor/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchid\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "suggest": {
    "orchid/socket": "Laravel library for asynchronously serving WebSockets."
  },
  "minimum-stability": "alpha",
  "prefer-stable": true
}
